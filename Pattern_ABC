public class Pattern_ABC {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//char c[] = {'A','B','C'};
		
		char i;
		for( i = 'A';i<'D';i++)
		{
			for(char j = 'A';j<'D';j++)
			{
				for(char k ='A'; k<'D';k++)
				{
					System.out.print(i);
					System.out.print(j);
					System.out.print(k+" ");
					
				}
				
			}
		}

	}

}
